<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxPanel" name="wxPanelVariable">
		<pos_arg>1</pos_arg>
		<size_arg>1</size_arg>
		<object class="wxBoxSizer" variable="BoxSizer1" member="no">
			<orient>wxVERTICAL</orient>
			<object class="sizeritem">
				<object class="wxBoxSizer" variable="BoxSizer2" member="no">
					<object class="sizeritem">
						<object class="Custom" name="ID_CUSTOM1" subclass="wxPanelInstance" variable="panelInstance" member="yes">
							<creating_code>$(THIS) = new $(CLASS)($(PARENT),$(ID));</creating_code>
							<style></style>
						</object>
						<flag>wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxEXPAND</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxBoxSizer" variable="boxSizerDataType" member="no">
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_lblDataType" variable="lblDataType" member="yes">
							<label>DataType</label>
							<size>150,-1</size>
							<minsize>150,-1</minsize>
							<maxsize>150,-1</maxsize>
							<style>wxALIGN_RIGHT</style>
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<minsize>150,-1</minsize>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxComboBox" name="ID_COMBOBOX1" variable="cboDataType" member="yes" />
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON1" variable="bntLookupDataType" member="yes">
							<label>...</label>
							<size>32,21</size>
							<minsize>32,21</minsize>
							<maxsize>32,-1</maxsize>
							<handler function="OnbntLookupDataTypeClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<minsize>32,-1</minsize>
						<option>1</option>
					</object>
				</object>
				<flag>wxEXPAND</flag>
				<border>5</border>
			</object>
		</object>
	</object>
</wxsmith>
